<% include templates/header %>

<!-- SLIDER -->
<div id="myCarousel" class="carousel slide" data-ride="carousel">

    <!-- Wrapper for slides -->
    <div class="carousel-inner">

        <% data.Banners.forEach(function(banner, i) { %>
        <% if(i == 0) { var item = "active"; } %>

        <% if(banner.status) { %>

        <div class="<%= item %> item" style="background-image: url(<%= banner.image %>); background-position: center;">

            <% if(currentUser) { %>
            <div>
                <a href="/banner">
                    <button type="button" class="btn btn-primary btn-circle">
                        <i class="glyphicon glyphicon-pencil"></i>
                    </button>
                </a>
            </div>
            <% } %>

            <div class="container">
                <div class="carousel-caption">
                    <h1><%= banner.title %></h1>
                    <p class="lead"><%= banner.description %></p>
                    <% if(banner.button == true) { %>
                    <a class="btn btn-default" href="<%= banner.buttonUrl %>">Learn more</a>
                    <% } %>
                </div>
            </div>
        </div>

        <% } %>

        <% }); %>

    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div id="firstToSecond"></div>

<section id="secondSection">
    <div class="container">
        <div class="row">
            <div class="noticias col-md-5">

                <h4 class="title"><span class="glyphicon glyphicon-list-alt"></span> Latest News</h4>
                <table id="tabelaNoticia">
                    <tbody>

                        <% data.News.forEach(function(news) { %>

                        <% var monthNames = ["Jan", "Fev", "Mar", "Abr", "Mai", "Jun", "Jul", "Ago", "Set", "Out", "Nov", "Dez"]; %>
                        <% var title = news.title; %>
                        <% var mes = monthNames[news.date.getMonth()]; %>
                        <% var id = news._id; %>

                        <tr>
                            <td class="data">
                                <h5 id="dia" class="noticiaDataDia"><%= news.date.getDate()%></h5>
                                <h6 id="mes" class="noticiaDataMes"><%= mes %></h6>
                            </td>
                            <td class="title"><a href="/noticias/<%= id %>">
                                    <p><%= title %></p>
                                </a></td>
                        </tr>

                        <% }); %>

                    </tbody>
                </table>

                <div class="row">
                    <a class="secondSectionBtn btn btn-sm btn-danger" href="/noticias">View all</a>
                </div>
            </div>

            <div class="agenda col-md-4">

                <h4 class="title"><span class="glyphicon glyphicon-calendar"></span> Accounting Calendar</h4>
                <div class="agendaContent">
                    <div class="month">
                        <ul>
                            <form class="changemonth" action="/agenda/changemonth" method="POST" autocomplete="off">
                                <li class="prev">
                                    <input id='prevMonth' type='hidden' name='changemonth'
                                        value='<%= date.getMonth()-1; %>-<%= date.getFullYear(); %>'/>
                                    <button type='submit' class='btn btn-danger'>&#10094;</button>
                                </li>
                            </form>

                            <form class="changemonth" action="/agenda/changemonth" method="POST" autocomplete="off">
                                <li class="next">
                                    <input id='nextMonth' type='hidden' name='changemonth'
                                        value='<%= date.getMonth()+1; %>-<%= date.getFullYear(); %>'/>
                                    <button type='submit' class='btn btn-danger'>&#10095;</button>
                                </li>
                            </form>

                            <li>
                                <% var monthNamesFull = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"]; %>
                                <span id="monthHeader"><%= monthNamesFull[date.getMonth()] %></span>

                                <br>
                                <span id="yearHeader" style="font-size:18px"><%= date.getFullYear(); %></span>
                            </li>
                        </ul>
                    </div>

                    <% var k = []; %>
                    <% var f = {}; %>
                    <% data.Calendar.forEach(function(calendar, i) { %>

                    <% var year = calendar.date.getUTCFullYear(); %>
                    <% var  month = calendar.date.getUTCMonth()+1; %>
                    <% var  day = calendar.date.getUTCDate(); %>

                    <% if (day < 10) {
                            day = '0' + day;
                            }
                            if (month < 10) {
                            month = '0' + month;
                            }
                        %>

                    <% k.push(calendar.date.getUTCDate()); %>
                    <% f[calendar.date.getUTCDate()] = year+ '-' + month + '-' +day; %>

                    <% }); %>


                    <ul class="weekdays">
                        <li>S</li>
                        <li>T</li>
                        <li>Q</li>
                        <li>Q</li>
                        <li>S</li>
                        <li>S</li>
                        <li>D</li>
                    </ul>

                    <ul class="days">
                        <% for (i = 0; i < nodeCalendar[0]; i++) { %>
                        <li></li>
                        <% } %>

                        <% var haveTask = 0; %>
                        <% for (i = 0; i < nodeCalendar[1]; i++) { %>
                        <% (k.indexOf(i+1) !== -1 ? haveTask++ : "")%>
                        <li>
                            <form class="getAgenda" action="/agenda/" method="POST">
                                <%- (k.indexOf(i+1) !== -1 ? "<input type='hidden' name='agenda' value='"+f[i+1]+"'></input>": "") %>
                                <%- (k.indexOf(i+1) !== -1 ? "<button type='submit' class='btn btn-danger'>": "") %>
                                <%= i+1 %>
                                <%- (k.indexOf(i+1) !== -1 ? "</button>": "") %>
                            </form>
                        </li>
                        <% } %>
                    </ul>

                </div>
                <div class="row">
                    <div id="loadingContent">
                        <div id="loadingBar" class="hidden"></div>
                    </div>

                    <form class="getAgenda" action="/agenda/" method="POST">
                        <input id='monthButtonInput' type='hidden' name='agenda' value='<%= date.getMonth() %>-<%= date.getFullYear() %>'/>
                        <button style="margin-top: 2px;" id='monthButton' type='submit'
                            class='<%= (haveTask == 0? "hidden" : "") %>  secondSectionBtn btn btn-sm btn-danger'><%= monthNamesFull[date.getMonth()] %></button>
                    </form>

                </div>

            </div>

            <div class="newsletter col-md-3">
                <div class="newsletterContent">
                    <h4 class="title"><span class="glyphicon glyphicon-envelope"></span> Receive our Newsletter</h4>

                    <form>
                        <div class="form-group">
                            <label for="email">E-mail</label>
                            <input type="email" class="form-control" id="email">
                        </div>
                        <div class="form-group">
                            <label for="pwd">Name</label>
                            <input type="text" class="form-control" id="name">
                        </div>

                        <button type="submit" class="btn btn-danger btn-lg">Register</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

