<script type="text/javascript">
    $(document).on('submit', ".getAgenda", function (e) {

        $('#loadingBar').removeClass('hidden');
        $(".calendar-content").empty();
        $('.calendar-d').html("<i style='font-size: 0.8em;'>Click on Item to see the description</i>");

        $form = $(this);
        var url = $form.attr('action');
        var valor = $form.children().attr('value');

        var formData = new FormData(this);

        $.ajax({

            url: url,
            type: 'POST',
            cache: false,
            data: formData,
            beforeSend: function () {
                console.log("loading");
            },

            success: function (data) {

                var obj = JSON.parse(data);
                var text;
                var date;

                obj.forEach(function (cal) {

                    var title = cal.title;
                    var desc = cal.description;
                    var mes = new Date(cal.monthRef);
                    var ano = mes.getFullYear();
                    date = dat(cal.date);
                    var id = cal._id;

                    text = text + "<tr style='text-align: center;'>\
                    <td style='width: 88px' class='noticiaTitulo'><p>" + date + "</p></td>\
                    <td class='noticiaTitulo'><a href='' class='agendaModalItem'><p value='" + desc + "'>" + title + "</p></a></td>\
                    <td style='width: 100px' class='noticiaTitulo'><p>" + dataMes(mes) + "/" + ano + "</p></td>\
                    </tr>";

                });
                // VERIFY IF DATE IS SPECIFIC, OR REPREST A WHOLE MONTH.
                if (valor.length <= 7) {
                    $(".modal-title").empty().html("Agenda Contábil - " + date.substring(3, 10));
                } else {
                    $(".modal-title").empty().html("Agenda Contábil - " + date);
                }

                $(".calendar-content").html(text);
                $("#modalAgenda").modal('show');
                $('#loadingBar').addClass('hidden');


            },
            error: function (data) {
                console.log(data);
            },

            processData: false,
            contentType: false,

        });

        e.preventDefault();
    });
</script>

<script type="text/javascript">
    $(document).on('submit', ".changemonth", function (e) {

        $("#loadingBar").removeClass('hidden');
        console.log(this);

        var form = $(this);
        var url = form.attr('action');
        var formData = new FormData(this);

        $.ajax({

            url: url,
            type: 'POST',
            cache: false,
            data: formData,
            beforeSend: function () {
                console.log("loading");
            },

            success: function (data) {

                var monthNamesFull = ["January", "February", "March", "April", "May", "June",
                    "July", "August", "September", "October", "November", "December"
                ];

                var k = [];

                var f = {};

                var text = "";

                var calDate = new Date(data.date);

                var monthName = monthNamesFull[calDate.getUTCMonth()];

                if (data.data.Calendar.length > 0) {

                    var calendarDate;

                    data.data.Calendar.forEach(function (calendar, i) {

                        calendarDate = new Date(calendar.date);

                        var year = calendarDate.getUTCFullYear();
                        var month = calendarDate.getUTCMonth() + 1;
                        var day = calendarDate.getUTCDate();

                        if (day < 10) {
                            day = '0' + day;
                        }
                        if (month < 10) {
                            month = '0' + month;
                        }

                        k.push(calendarDate.getUTCDate());

                        f[calendarDate.getUTCDate()] = year + '-' + month + '-' + day;

                    });
                }

                for (i = 0; i < data.nodeCalendar[0]; i++) {
                    text = text + "<li></li>"
                }

                for (i = 0; i < data.nodeCalendar[1]; i++) {
                    text = text + "<li><form class='getAgenda' action='/agenda/' method='POST'>";
                    if (k.indexOf(i + 1) !== -1) {
                        text = text + "<input type='hidden' name='agenda' value='" + f[i + 1] + "'/>\
                                        <button type='submit' class='btn btn-danger'>"
                    }
                    text = text + (i + 1)
                    if (k.indexOf(i + 1) !== -1) {
                        text = text + "</button>"
                    }
                    text = text + "</form></li>"

                }


                if (k.length == 0) {
                    $("#monthButton").addClass("hidden");
                } else {
                    $("#monthButton").removeClass("hidden");
                }

                $("ul.days").empty().html(text);
                $('#loadingBar').addClass('hidden');
                $("#monthHeader").empty().html(monthName);
                $("#monthButton").empty().html(monthName);
                $("#yearHeader").empty().html(calDate.getUTCFullYear());
                $("#monthButtonInput").val((calDate.getUTCMonth()) + "-" + calDate
            .getUTCFullYear());
                $("#prevMonth").val((calDate.getUTCMonth() - 1) + "-" + calDate.getUTCFullYear());
                $("#nextMonth").val((calDate.getUTCMonth() + 1) + "-" + calDate.getUTCFullYear());
            },
            error: function (data) {
                console.log(data);
            },

            processData: false,
            contentType: false,


        });

        e.preventDefault();
    });
</script>

