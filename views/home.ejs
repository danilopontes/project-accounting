<script>
    $('.scrollLeft').mouseover(function (event) {

        var maxscroll = $(".parceirosContainer").get(0).scrollWidth;
        var speed = maxscroll * 2;

        $(".parceirosContainer").animate({
            scrollLeft: -maxscroll
        }, speed, "linear");

    });
    $('.scrollRight').mouseover(function (event) {

        var maxscroll = $(".parceirosContainer").get(0).scrollWidth;
        var speed = maxscroll * 2;

        $(".parceirosContainer").animate({
            scrollLeft: maxscroll
        }, speed, "linear");

    });

    $('.scrollLeft, .scrollRight').mouseout(function (event) {
        $('.parceirosContainer').stop();

    });
</script>


